<div class="contentt">
  <%= render 'shared/errors', obj: @user %>
  <%= form_with(url: '/users', model: @user, local: true ) do |f| %>
  <div class="ui form">
    <div class="field">
      <label><%= f.label :username %></label>
      <%= f.text_field :username %>
    </div>
    <div class="field">
      <label><%= f.label :email %></label>
      <%= f.text_field :email %>
    </div>
    <div class="field">
      <label><%= f.label :password %></label>
      <%= f.password_field :password %>
    </div>
    <%= f.submit 'Sign Up', :class => 'ui submit button large fluid' %>
  </div>
  <% end %>
</div>